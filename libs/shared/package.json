{
  "name": "@fss/shared",
  "author": "Ilya Kushlianski",
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "module": "dist/shared.esm.js",
  "size-limit": [
    {
      "path": "dist/shared.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/shared.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "4.7.0",
    "husky": "4.3.0",
    "size-limit": "4.7.0",
    "tsdx": "0.14.1",
    "tslib": "2.0.3",
    "typescript": "4.0.5"
  }
}
